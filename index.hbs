{{!< default}}

<div class="posts">
  {{#foreach posts}}
  <div class="post">
    <h1 class="post-title">
      <a href="https://roguehacklab.com/blog{{url}}">
        {{{title}}}
      <span class="see-more">&nbsp;&nbsp;Full Post ... </span>
      </a>
    </h1>

    <span class="post-date">{{date format='YYYY-MM-DD'}}</span>

    {{content}}
  </div>
  {{/foreach}}

  {{pagination}}
</div>
